{
  "name": "rugalib/ruga-baseurl",
  "description": "Baseurl component for rugalib",
  "license": "AGPL-3.0-only",
  "homepage": "https://github.com/rugalib",
  "type": "library",
  "require": {
    "php": "^7.4|^8.0|^8.1|^8.2",
    "psr/http-message": "^1.0",
    "psr/http-server-handler": "^1.0",
    "psr/http-server-middleware": "^1.0",
    "laminas/laminas-view": "^2.23"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.6",
    "laminas/laminas-config-aggregator": "^1.7",
    "laminas/laminas-servicemanager": "^3.11"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.rugalib.ch/"
    }
  ],
  "provide": {
    "mtymek/blast-base-url": "*"
  },
  "authors": [
    {
      "name": "Roland Rusch",
      "email": "roland.rusch@easy-smart.ch"
    }
  ],
  "autoload": {
    "psr-4": {
      "Ruga\\Baseurl\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ruga\\Baseurl\\Test\\": "test/src"
    }
  },
  "extra": {
    "laminas": {
      "component": "Ruga\\Baseurl",
      "config-provider": "Ruga\\Baseurl\\ConfigProvider"
    }
  }
}